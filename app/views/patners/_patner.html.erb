<div id="<%= dom_id patner %>">
  <tr>
    <td><%= patner.name %></td>
    <td><%= patner.code %></td>
    <td>
      <%= link_to patner_path(patner), class: 'btn btn-secondary btn-sm' do %>
        <i class="fas fa-eye"></i> Show
      <% end %>
      <%= link_to edit_patner_path(patner), class: 'btn btn-primary btn-sm ml-2' do %>
        <i class="fas fa-pencil-alt"></i> Edit
      <% end %>
      <%= link_to patner_path(patner), data: { turbo_method: :delete, turbo_confirm: 'Are you sure?' }, class: 'btn btn-danger btn-sm ml-2' do %>
        <i class="fas fa-trash"></i> Delete
      <% end %>
    </td>
  </tr>
</div>